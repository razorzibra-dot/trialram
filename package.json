{
  "name": "crm-portal",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:modular": "vite",
    "build": "tsc && vite build",
    "build:modular": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:modular": "echo 'Testing modular architecture...' && npm run dev",
    "validate:code": "node scripts/validate-code.cjs --verbose",
    "validate:staged": "node scripts/validate-code.cjs --staged --verbose",
    "validate:pre-commit": "node scripts/validate-code.cjs --pre-commit",
    "fix:unicode-escapes": "node scripts/validate-code.cjs --fix --verbose",
    "fix:staged": "node scripts/validate-code.cjs --fix --staged",
    "quality:check": "npm run lint && npm run validate:code",
    "quality:fix": "npm run lint --fix && npm run fix:unicode-escapes",
    "prepare": "husky install || true",
    "seed:auth": "tsx scripts/seed-auth-users.ts",
    "seed:sync": "tsx scripts/sync-auth-to-sql.ts",
    "seed:sql": "tsx scripts/generate-seed-sql.ts > supabase/seed-users.sql",
    "seed:db": "tsx scripts/seed-database.ts",
    "seed:reset": "supabase db reset",
    "seed:all": "npm run seed:auth && npm run seed:sync && npm run seed:db"
  },
